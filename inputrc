set editing-mode vi
set show-mode-in-prompt on
set vi-cmd-mode-string "\1\e[2 q\2cmd)"
set vi-ins-mode-string "\1\e[1 q\2ins)"
set completion-ignore-case on
"jj": vi-movement-mode
"qq": menu-complete
# optionally:
# switch to block cursor before executing a command
set keymap vi-insert
RETURN: "\e\n"
